<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- When selected, use filled up icon -->
    <item android:drawable="@drawable/ic_fav_selected_icon" android:state_checked="true" />
    <!-- When not selected, use border icon-->
    <item android:drawable="@drawable/ic_fav_unselected_icon" android:state_checked="false" />

</selector>